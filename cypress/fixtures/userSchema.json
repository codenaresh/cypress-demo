{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User Schema",
  "type": "object",
  "required": ["id", "name", "email", "active"],
  "additionalProperties": false,
  "properties": {
    "id": { "type": "number" },
    "name": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "active": { "type": "boolean" }
  }
}
